{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "Orders"
    },
    "Products": {
      "type": "json"
    },
    "PaymentMethod": {
      "type": "enumeration",
      "enum": [
        "card",
        "paypal"
      ],
      "default": "card"
    },
    "Stage": {
      "type": "enumeration",
      "enum": [
        "Received",
        "Packing",
        "Sended",
        "Delivered",
        "Canceled"
      ],
      "default": "Received"
    },
    "TotalPrice": {
      "type": "decimal"
    },
    "DeliveryAddress": {
      "type": "string"
    },
    "Country": {
      "type": "string"
    },
    "City": {
      "type": "string"
    }
  }
}
